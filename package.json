{
    "name": "zsharp",
    "displayName": "ZSharp",
    "description": "This is used to get some syntax highlighting for ZSharp, it was made in a day, so it's not perfect, but it works.",
    "publisher": "StevenRafft",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "zs",
            "aliases": ["ZSharp", "zs"],
            "extensions": ["zs"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "zs",
            "scopeName": "source.zsharp",
            "path": "./syntaxes/zs.tmLanguage.json"
        }]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/StevenRafft/zsharp-syntax-highlighting"
    }
}